{
  "presets": ["@babel/preset-env", "@babel/react"],
    "env": {
      "test": {
        "presets": ["@babel/preset-env", "@babel/react"]
      }
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "transform-es2015-modules-commonjs",
      "@babel/plugin-proposal-object-rest-spread",
      ["@babel/plugin-proposal-decorators", { "legacy": true }]
    ]
  }
